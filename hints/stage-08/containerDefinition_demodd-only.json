{
    "family": "demoapp",
    "containerDefinitions": [
        {
             "name": "demodb",
             "image": "mysql",
             "memory": 500,
             "cpu": 10,
             "portMappings": [{
                "containerPort": 3306,
                "hostPort": 3306
             }],
             "environment": [
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "mysql"
                },
                {
                    "name": "MYSQL_DATABASE",
                    "value": "demodb"
                }
            ]
        }
    ],
    "volumes": [],
    "networkMode": "bridge",
    "placementConstraints": []
}